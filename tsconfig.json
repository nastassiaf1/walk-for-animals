{
    "compilerOptions": {
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowUmdGlobalAccess": true,
        "baseUrl": "./",
        "paths": {
          "*": ["src/*"]
        },
    },
    "include": ["src/**/*.ts",  "src/**/*.tsx"]
}
